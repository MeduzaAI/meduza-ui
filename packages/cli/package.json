{
  "name": "@meduza-ui/cli",
  "version": "0.1.0",
  "description": "Command-line interface for meduza-ui component installation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "meduza-ui": "dist/index.js"
  },
  "files": ["dist/**/*", "README.md"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@meduza-ui/core": "workspace:*",
    "commander": "^12.1.0",
    "prompts": "^2.4.2",
    "chalk": "^5.3.0",
    "ora": "^8.1.1",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.10",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
